<div class="container-fluid container-fluid-searchnote pb-5 mb-4">
  <div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
    <% if (results.length > 0) { %>
      <% results.forEach((note, index) => { %>
        <a href="/dashboard/notes/<%= note._id %>">
          <h3>Note: <%= note.title %></h3>
        </a>
      <% }); %>
    <% } else { %>
      <p>No notes found.</p>
    <% } %>
  </div>
</div>